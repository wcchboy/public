<!-- checked_checkbox.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/> <!-- 设置圆角半径 -->
            <stroke android:width="1dp" android:color="#FF3AB1EF"/> <!-- 设置边框宽度和颜色 -->
            <solid android:color="#FF04418A"/> <!-- 设置填充颜色 -->
        </shape>
    </item>
    <item android:drawable="@drawable/checkbox_check_icon"/> <!-- 使用自定义的√图标 -->
</layer-list>
